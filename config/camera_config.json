{
    "cam_name": "PiSecCam",
    "framerate": 20,
    "rotation": 180,
    "video_size": [
        1280,
        720
    ],
    "dropbox": {
        "file_chunk_megs": 3,
        "token": null
    },
    "formats": {
        "overlay_date_format": "%Y-%m-%d %H:%M:%S",
        "day_directory_format": "%Y-%m-%d",
        "time_directory_format": "%H.%M.%S"
    },
    "motion": {
        "max_event_time": 60,
        "min_trigger_area": 0.005,
        "min_pixel_delta_trigger": 50,
        "rec_sec_after": 8,
        "rec_sec_before": 8
    },
    "server": {
        "enabled": true,
        "api_key": null,
        "certfile_path": null,
        "keyfile_path": null,
        "mjpeg_framerate_cap": 1.5,
        "server_port": 8080
    }
}
