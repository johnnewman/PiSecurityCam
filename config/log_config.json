{
    "formatters": {
        "default": {
            "format": "%(asctime)s  %(levelname)-6s %(name)-10s %(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "formatter": "default",
            "level": 20
        },
        "file": {
            "backupCount": 7,
            "class": "logging.handlers.TimedRotatingFileHandler",
            "filename": "logs/pyseccam.log",
            "formatter": "default",
            "interval": 1,
            "level": 20,
            "when": "d"
        }
    },
    "root": {
        "handlers": [
            "console",
            "file"
        ],
        "level": 20
    },
    "version": 1
}
